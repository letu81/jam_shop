<script type="text/javascript">
    function code() {
        $(".codebox").show();
    }

    function closes() {
        $(".codebox").hide();
    }

    $(function () {
        $.qReport(1);
        $.stellar({
            horizontalScrolling: false,
            verticalOffset: 40
        });
        var firstHeight = 960;
        $(window).scroll(function (e) {
            scrollTop = $(window).scrollTop();
            if (scrollTop >= firstHeight) {
                $("#topbar").addClass("topicfixed");
                $("#topbar .logo a").removeClass("white");
                $("#topbar .logo a").addClass("blue");
            } else {
                if ($("#topbar").hasClass("topicfixed")) {
                    $("#topbar").removeClass("topicfixed");
                    $("#topbar .logo a").removeClass("blue");
                    $("#topbar .logo a").addClass("white");
                }
            }

        })
    });
</script>

